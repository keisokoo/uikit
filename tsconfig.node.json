{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["esnext"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "include": ["scripts/**/*"],
  "exclude": ["node_modules"]
}
